{
  "1": {
    "class_type": "HyVideoModelLoader",
    "inputs": {
      "model_name": "hyvideo/hunyuan_video_720_fp8_e4m3fn.safetensors",
      "precision": "bf16",
      "attn_mode": "sageattn_varlen",
      "offload": "offload_device"
    }
  },
  "3": {
    "class_type": "HyVideoSampler",
    "inputs": {
      "model": ["1", 0],
      "hyvid_embeds": ["30", 0],
      "width": |width|,
      "height": |height|,
      "num_frames": |num_frames|,
      "steps": |steps|,
      "cfg": 1.0
    }
  },
  "7": {
    "class_type": "HyVideoVAELoader",
    "inputs": {
      "vae_name": "hyvid/hunyuan_video_vae_bf16.safetensors",
      "precision": "fp16"
    }
  },
  "16": {
    "class_type": "DownloadAndLoadHyVideoTextEncoder",
    "inputs": {
      "llm_path": "Kijai/llava-llama-3-8b-text-encoder-tokenizer",
      "clip_path": "openai/clip-vit-large-patch14",
      "precision": "fp16"
    }
  },
  "30": {
    "class_type": "HyVideoTextEncode",
    "inputs": {
      "text_encoders": ["16", 0],
      "text": "|prompt|",
      "negative": "|negative_prompt|"
    }
  },
  "34": {
    "class_type": "VHS_VideoCombine",
    "inputs": {
      "images": ["5", 0],
      "fps": |fps|,
      "format": "video/h264-mp4",
      "filename_prefix": "HunyuanVideo"
    }
  },
  "5": {
    "class_type": "HyVideoDecode",
    "inputs": {
      "vae": ["7", 0],
      "samples": ["3", 0]
    }
  }
}